{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Arduino CLI: Verify",
            "type": "shell",
            "command": "arduino-cli compile --fqbn arduino:avr:uno ${file}",
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            }
        },
        {
            "label": "Arduino CLI: Upload",
            "type": "shell",
            "command": "arduino-cli upload -p COM3 --fqbn arduino:avr:uno ${file}",
            "problemMatcher": [
                "$gcc"
            ],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            }
        }
    ]
}